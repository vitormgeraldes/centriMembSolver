    }
    membraneB
    {
        type    membraneSoluteFlux;
        model   intrinsicRejection;
        Rint    0.99;
        UName   U;
        CAb     1.0;     // required only when writeRobs=yes
        value   uniform 0;

        // type    membraneSoluteFlux;
